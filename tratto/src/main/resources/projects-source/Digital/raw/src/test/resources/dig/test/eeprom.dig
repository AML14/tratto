            <dataString>A   CS OE WE D_in   D_out
0   0  0  0  Z      Z

# write zeros to EEPROM
loop(n,30)
(n) 1  0  1  0      0
(n) 1  0  0  0      0
end loop

# check its cleared
loop(n,30)
(n) 1  1  0  Z      0
end loop


# write data to EEPROM
loop(n,30)
(n) 1  0  1  (n)    (n)
(n) 1  0  0  (n)    (n)
end loop

# read data from EEPROM
loop(n,30)
(n) 1  1  0  Z      (n)
end loop
</dataString>
